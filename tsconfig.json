{
  "compilerOptions": {
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "outDir": "lib",
    "strictNullChecks": true,
    "esModuleInterop": true,
    "noImplicitOverride": true,
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "Node",
    "jsx": "react-jsx",
    "skipLibCheck": true
  },
  "include": ["src", "vite.config.ts", "codegen.ts"],
  "exclude": ["src/**/template-*"]
}
